{
  "name": "react-perfect-zoom",
  "version": "0.1.0",
  "main": "dist/index.js",
  "private": false,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jedluk/react-perfect-zoom"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bugs": {
    "url": "https://github.com/jedluk/react-perfect-zoom/issues"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "NODE_ENV=production && rm -rf dist && mkdir dist && npx babel src --ignore '**/*.spec.js' --out-dir dist --copy-files",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "npm:publish": "CI=true npm run test && npm run build && npm run publish"
